    "@release-it/conventional-changelog": "^1.1.0",
    "@types/jest": "23.3.9",
    "@types/puppeteer": "1.9.1",
    "conventional-changelog-cli": "^2.0.23",
    "conventional-changelog": "^3.1.10",
    "jest": "23.6.0",
